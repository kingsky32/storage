{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {
      "#/*": ["*"],
      "#authentication/*": ["./authentication/*"],
      "#commons/*": ["./commons/*"],
      "#configs/*": ["./configs/*"],
      "#models/*": ["./models/*"],
      "#providers/*": ["./providers/*"]
    },
    "incremental": true,
    "skipLibCheck": true
  }
}
